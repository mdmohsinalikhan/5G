\begin{tikzpicture}[scale=5]



\filldraw[draw=black,fill=white,line width = 3pt] (0,-0.25) rectangle (1.5,0.75);
\draw [black, line width = 4pt, <->] (0,.95) -- (1.5,.95);
\draw node at (.75,1.1) {3 digits};
\draw node at (.75,.5) {\textbf{MCC}};
\draw node at (.75,.2) {Mobile};
\draw node at (.75,0) {Country Code};

\filldraw[draw=black,fill=white,line width = 3pt] (1.9,-0.25) rectangle (3.55,0.75);
\draw [black, line width = 4pt, <->] (1.9,0.95) -- (3.55,0.95);
\draw node at (2.75,1.1) {2 or 3 digits};
\draw node at (2.75,.5) {\textbf{MNC}};
\draw node at (2.75,.2) { Mobile};
\draw node at (2.75,.0) { Network Code};

\filldraw[draw=black,fill=white,line width = 3pt] (4,-0.25) rectangle (7.5,0.75);
\draw [black, line width = 4pt, <->] (4,0.95) -- (7.5,0.95);
\draw node at (5.75,1.1) {9 or 10 digits};
\draw node at (5.75,.5) {\textbf{MSIN}};
\draw node at (5.75,.2) {Mobile Subscription};
\draw node at (5.75,.0) {Identification Number};

\draw node at (3.5,-.9) {\textbf{IMSI}};
\draw node at (3.5,-1.2) {15 decimal digits};


\draw [black, line width = 4pt, <->] (0,-0.6) -- (7.5,-0.6);

\end{tikzpicture}

